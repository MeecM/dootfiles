! Xclock colors.
XClock*foreground: #a0bec6
XClock*background: #010202
XClock*majorColor:  rgba:a0/be/c6/ff
XClock*minorColor:  rgba:a0/be/c6/ff
XClock*hourColor:   rgba:a0/be/c6/ff
XClock*minuteColor: rgba:a0/be/c6/ff
XClock*secondColor: rgba:a0/be/c6/ff

! Set transparency.
!URxvt*depth: 32
URxvt.transparent:    false
!URxvt.shading:       10
! Hide ugly bar.
URxvt.scrollBar: false
! Set font.
xterm*font: xft:Hack Powerline:size=10
URxvt.font: xft:Hack Powerline:size=11
xterm*saveLines: 2000


